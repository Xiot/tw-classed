/// <reference types="react" />
import type { ClassNames, TwModifyer, Variants, ClassNamesAndVariant, InferVariantProps, ClassedComponent, VariantProps } from "./types.js";
declare function classed<T extends keyof JSX.IntrinsicElements | React.ComponentType<any>, V extends Variants = {}>(elementType: T, ...classNames: ClassNamesAndVariant<V>[]): ClassedComponent<T, V>;
export default classed;
export type { ClassNames, TwModifyer, InferVariantProps, ClassedComponent, VariantProps, };
